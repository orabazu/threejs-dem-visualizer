{
    "name": "threejs-es6-webpack-starter-landsat",
    "version": "0.0.1",
    "description": "A starter project for Three.js with support for ES6 and Webpack 4",
    "scripts": {
        "build": "webpack --config webpack.prod.js --progress --env production",
        "start": "webpack serve --config webpack.dev.js --env development --open",
        "ci": "yarn run test && yarn run coverage",
        "coverage": "codecov",
        "deploy": "npx gh-pages -d build",
        "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx}'",
        "nuke": "rimraf node_modules && rm yarn.lock",
        "predeploy": "yarn build",
        "test": "jest --verbose",
        "lint": "eslint ./src --ext .js ",
        "lint:fix": "eslint ./src --ext .js --fix",
        "analyze-bundle": "npx webpack --json > stats.json && yarn webpack-bundle-analyzer stats.json"
    },
    "repository": "https://github.com/zhunor/threejs-dem-visualizer",
    "author": "zhunor <ozerorhun@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "orbit-controls-es6": "2.0.0",
        "three": "0.130.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/eslint-parser": "^7.14.7",
        "@babel/preset-env": "^7.14.8",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.0.6",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "compression-webpack-plugin": "^8.0.1",
        "css-loader": "^6.2.0",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-webpack-plugin": "^3.0.1",
        "file-loader": "^6.2.0",
        "gh-pages": "^3.2.3",
        "html-webpack-plugin": "^5.3.2",
        "image-webpack-loader": "^7.0.1",
        "jest": "^27.0.6",
        "jest-dom": "^4.0.0",
        "mini-css-extract-plugin": "^2.1.0",
        "node-sass": "^6.0.1",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "sass-lint": "^1.13.1",
        "sass-loader": "^12.1.0",
        "style-loader": "^3.2.1",
        "webpack": "^5.46.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2",
        "webpack-glsl-loader": "1.0.1",
        "webpack-merge": "^5.8.0"
    },
    "keywords": [
        "threejs",
        "three.js",
        "boilerplate",
        "webpack",
        "es6",
        "babel",
        "sass"
    ],
    "bugs": {
        "url": "https://github.com/zhunor/threejs-dem-visualizer/issues"
    },
    "homepage": "https://github.com/zhunor/threejs-dem-visualizer"
}
